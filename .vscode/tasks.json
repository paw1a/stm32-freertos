  "version": "2.0.0",
  "tasks": [
    {
      "label": "Build GD32F450 Debug",
      "type": "shell",
      "command": "mingw32-make MCU_TYPE=GD32F450 Debug",
      "problemMatcher": "$gcc",
      "group": {
        "kind": "build",
        "isDefault": false
      }
    },
    {
      "label": "Build MK61 Debug",
      "type": "shell",
      "command": "mingw32-make MCU_TYPE=MK61 Debug",
      "problemMatcher": "$gcc",
      "group": {
        "kind": "build",
        "isDefault": false
      }
    },
    {
      "label": "Build MKL82 Debug",
      "type": "shell",
      "command": "mingw32-make MCU_TYPE=MKL82 Debug",
      "problemMatcher": "$gcc",
      "group": {
        "kind": "build",
        "isDefault": false
      }
    },
    {
      "label": "Build GD32F450 Release",
      "type": "shell",
      "command": "mingw32-make MCU_TYPE=GD32F450 Release",
      "problemMatcher": "$gcc",
      "group": {
        "kind": "build",
        "isDefault": false
      }
    },
    {
      "label": "Build MK61 Release",
      "type": "shell",
      "command": "mingw32-make MCU_TYPE=MK61 Release",
      "problemMatcher": "$gcc",
      "group": {
        "kind": "build",
        "isDefault": false
      }
    },
    {
      "label": "Clean",
      "type": "shell",
      "options": {
        "cwd": "."
      },
      "command": "mingw32-make clean",
      "problemMatcher": "$gcc",
      "group": {
        "kind": "build",
        "isDefault": false
      }
    },
    {
      "label": "Clean except MAL",
